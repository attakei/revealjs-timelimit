{
  "name": "revealjs-timelimit",
  "version": "0.1.0",
  "description": "Reveal.js plugin to interrupt presentations forcely when time is over",
  "scripts": {
    "format": "prettier --write src/",
    "build:browser": "rollup src/plugin.js --file dist/timelimit.js --format iife --name RevealTimelimit",
    "build:commonjs": "rollup src/plugin.js --file dist/timelimit.cjs.js --format cjs --name RevealTimelimit",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attakei/revealjs-timeout.git"
  },
  "keywords": [
    "Reveal.js"
  ],
  "author": "Kazuya Takei",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/attakei/revealjs-timeout/issues"
  },
  "homepage": "https://github.com/attakei/revealjs-timeout#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0"
  }
}
